{
  "name": "ts-patch-mongoose",
  "version": "2.2.0",
  "description": "Patch history & events for mongoose models",
  "author": "Alex Eagle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ilovepixelart/ts-patch-mongoose.git"
  },
  "bugs": {
    "url": "https://github.com/ilovepixelart/ts-patch-mongoose/issues"
  },
  "homepage": "https://github.com/ilovepixelart/ts-patch-mongoose#readme",
  "directories": {
    "examples": "examples"
  },
  "keywords": [
    "backend",
    "mongoose",
    "plugin",
    "mongo",
    "schema",
    "db",
    "nosql",
    "ts",
    "typescript",
    "swc",
    "patch",
    "history",
    "event",
    "emit",
    "create",
    "update",
    "delete",
    "audit",
    "log"
  ],
  "engines": {
    "node": "^14.15.0 || ^16.10.0 || >=18.0.0"
  },
  "files": [
    "dist",
    "src",
    "tests",
    ".swcrc",
    "tsconfig.json",
    "jest.config.ts",
    "jest-mongodb-config.ts",
    ".eslintrc",
    ".eslintignore"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/plugin.d.ts",
        "default": "./dist/esm/plugin.mjs"
      },
      "require": {
        "types": "./dist/cjs/types/plugin.d.ts",
        "default": "./dist/cjs/plugin.js"
      }
    }
  },
  "main": "dist/cjs/plugin.js",
  "types": "dist/cjs/types/plugin.d.ts",
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "test": "jest --config jest.config.ts --detectOpenHandles --coverage",
    "test:open": "npm run test && open-cli coverage/lcov-report/index.html",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p config/tsconfig.cjs.json",
    "build:esm": "tsc -p config/tsconfig.esm.json && mv ./dist/esm/plugin.js ./dist/esm/plugin.mjs",
    "release": "npm install && npm run lint && npm run build && np"
  },
  "dependencies": {
    "fast-json-patch": "3.1.1",
    "lodash": "4.17.21",
    "omit-deep": "0.3.0",
    "power-assign": "0.2.10",
    "semver": "7.5.4"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "4.1.7",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.78",
    "@swc/helpers": "0.5.1",
    "@swc/jest": "0.2.29",
    "@swc/register": "0.1.10",
    "@types/jest": "29.5.4",
    "@types/lodash": "4.14.197",
    "@types/node": "18",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "eslint": "8.47.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-sonarjs": "0.20.0",
    "jest": "29.6.3",
    "merge": "2.1.1",
    "mongoose": "6.11.4",
    "open-cli": "7.2.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "mongoose": ">=6.6.0 < 8"
  }
}
